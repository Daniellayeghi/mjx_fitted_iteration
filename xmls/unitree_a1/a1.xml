        <default class="thigh2">
          <geom contype="0" conaffinity="0" group="3" size="0.015" fromto="0 0 0 -0.02 0 -0.1"/>
        </default>
        <default class="thigh3">
          <geom contype="0" conaffinity="0" group="3" size="0.015" fromto="-0.02 0 -0.16 0 0 -0.2"/>
        </default>
        <default class="calf1">
          <geom contype="0" conaffinity="0" group="3" size="0.01" fromto="0 0 0 0.02 0 -0.13"/>
        </default>
        <default class="calf2">
          <geom contype="0" conaffinity="0" group="3" size="0.01" fromto="0.02 0 -0.13 0 0 0."/>
