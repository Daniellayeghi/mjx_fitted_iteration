<mujoco model="Quadruped Terrain">
  <include file="common.xml"/>
  <!-- <option impratio="10" cone="elliptic"/> -->
  <size memory="1M"/>

  <custom>
    <text name="task_transition" data="Loop|Stage1|Stage2|Stage3|Stage4|Stage5|Stage6|Stage7|Stage8|Stage9|Stage10|Stage11|Stage12|Stage13|Stage14|Stage15|Stage16|Stage17|Stage18|Stage19" />
    <numeric name="agent_planner" data="0" />
    <numeric name="agent_horizon" data="0.25" />
    <numeric name="agent_timestep" data="0.01" />
    <numeric name="sampling_spline_points" data="5" />
    <numeric name="sampling_exploration" data="0.6" />
    <numeric name="gradient_spline_points" data="5" />
    <numeric name="sampling_trajectories" data="128" />
    <numeric name="gradient_num_trajectory" data="64" />
    <numeric name="ilqg_num_rollouts" data="8" />
    <numeric name="ilqg_regularization_type" data="1" />
    <numeric name="ilqg_representation" data="2" />
    <numeric name="residual_nn_updated" size="1" data="-1." />
    <numeric name="residual_nn" size="6" data="0.5 0. 0. 0. 0. 0.33333" />
    <numeric name="residual_nn_reset" size="3" data="0. 0. 0." />
    <numeric name="residual_yaw_local" size="1" data="0." />
    <numeric name="residual_Height" data="0.25" />
    <numeric name="residual_air_time_limit" size="1" data="0.4" />
    <numeric name="residual_air_time_time0" size="1" data="0." />
    <numeric name="residual_air_time_FR" size="1" data="0." />
    <numeric name="residual_air_time_FL" data="0." />
    <numeric name="residual_air_time_HR" data="0." />
    <numeric name="residual_air_time_HL" data="0." />
    <numeric name="residual_ctrl_factor_hip" size="1" data="2." />
  </custom>

  <worldbody>
    <light pos="0 0 1.5" dir="0 0 -1" diffuse="0.7 0.7 0.7" specular="0.3 0.3 0.3"
        directional="true" castshadow="true"/>
    <!-- <body name="goal" mocap="true" pos="-0.125 -0.625 0.25" quat="0.7071 0 0 -0.7071">
        <geom type="capsule" size="0.1 0.1 0.05" contype="0" conaffinity="0" rgba="0 1 0 .02" fromto="-0.1 0 0 0.1 0 0"/>
    </body> -->
    <geom name="floor" size="50 50 0.05" pos="0 0 -0.01" type="plane" material="blue_grid"/>

    <!-- <body name="box" mocap="true" pos="-2.5 0 0">
      <geom name="box" class="prop" size="1 1 0.3"/>
    </body> -->
    <!-- <geom name="ramp" class="prop" pos="3.13 2.5 -.18" size="1.6 1 .5" euler="0 -0.2 0"/> -->
    <!-- <geom name="hill" class="prop" pos="0 6 -5.5" size="6" type="sphere"/> -->

    <!-- <geom name="box1" class="prop" pos="0 0 -.4" size="0.47 1 .4" euler="0 0 0" type="box"/>
    <geom name="box2" class="prop" pos="2.7 0 -.4" size="2. 1 .4" euler="0 0 0" type="box"/> -->
  </worldbody>

  <include file="a1.xml" />

  <default>
    <default class="torque">
      <general gainprm="40" ctrllimited="true" ctrlrange="-1 1"/>
    </default>
    <default class="task">
      <site rgba="1 0 0 1" size=".02" group="5"/>
      <default class="flight">
        <site rgba="1 .3 .3 1" size="0.019" group="5"/>
      </default>
      <default class="stance">
        <site rgba=".3 1 .3 1" size=".018" group="4"/>
      </default>
    </default>
    <default class="prop">
      <geom type="box" rgba="0 0.4 1 1"/>
    </default>
  </default>

  <!-- position actuators -->
  <actuator>
    <position class="abduction" name="FR_hip" joint="FR_hip_joint"/>
    <position class="hip" name="FR_thigh" joint="FR_thigh_joint"/>
    <position class="knee" name="FR_calf" joint="FR_calf_joint"/>
    <position class="abduction" name="FL_hip" joint="FL_hip_joint"/>
    <position class="hip" name="FL_thigh" joint="FL_thigh_joint"/>
    <position class="knee" name="FL_calf" joint="FL_calf_joint"/>
    <position class="abduction" name="RR_hip" joint="RR_hip_joint"/>
    <position class="hip" name="RR_thigh" joint="RR_thigh_joint"/>
    <position class="knee" name="RR_calf" joint="RR_calf_joint"/>
    <position class="abduction" name="RL_hip" joint="RL_hip_joint"/>
    <position class="hip" name="RL_thigh" joint="RL_thigh_joint"/>
    <position class="knee" name="RL_calf" joint="RL_calf_joint"/>
  </actuator>
  <!-- <actuator>
    <general class="torque" name="FR_hip" joint="FR_hip_joint"/>
    <general class="torque" name="FR_thigh" joint="FR_thigh_joint"/>
    <general class="torque" name="FR_calf" joint="FR_calf_joint"/>
    <general class="torque" name="FL_hip" joint="FL_hip_joint"/>
    <general class="torque" name="FL_thigh" joint="FL_thigh_joint"/>
    <general class="torque" name="FL_calf" joint="FL_calf_joint"/>
    <general class="torque" name="RR_hip" joint="RR_hip_joint"/>
    <general class="torque" name="RR_thigh" joint="RR_thigh_joint"/>
    <general class="torque" name="RR_calf" joint="RR_calf_joint"/>
    <general class="torque" name="RL_hip" joint="RL_hip_joint"/>
    <general class="torque" name="RL_thigh" joint="RL_thigh_joint"/>
    <general class="torque" name="RL_calf" joint="RL_calf_joint"/>
  </actuator> -->

  <!-- <include file="assets/fractal.xml" /> -->

  <sensor>
    <user name="Fly-high" dim="12" user="9 0.02 0.0 1.0 90. 0.5 0.9" />
    <user name="Control" dim="12" user="0 0.3 0.0 1.0" />
    <user name="Body Vel" dim="3" user="0 2. 0. 1.0 90." />
    <user name="Body Rot." dim="9" user="0 5. 0.0 10.0" />
    <user name="Body Ang vel" dim="3" user="0 1. 0.0 1.0 90." />
    <user name="Symmetry" dim="4" user="0 0.05 0.0 1.0" />
    <!-- <user name="Air-time-cost" dim="4" user="5 1000. 0.0 1.0 2 2" /> -->

    <framequat     name="orientation" objtype="xbody" objname="trunk"/>

    <framelinvel   name="FR_vel" objtype="site" objname="FR" reftype="world" refname="world"/>
    <framelinvel   name="FL_vel" objtype="site" objname="FL" reftype="world" refname="world"/>
    <framelinvel   name="HR_vel" objtype="site" objname="RR" reftype="world" refname="world"/>
    <framelinvel   name="HL_vel" objtype="site" objname="RL" reftype="world" refname="world"/>


    <framepos      name="FR" objtype="site" objname="FR" reftype="world" refname="world"/>
    <framepos      name="FL" objtype="site" objname="FL" reftype="world" refname="world"/>
    <framepos      name="HR" objtype="site" objname="RR" reftype="world" refname="world"/>
    <framepos      name="HL" objtype="site" objname="RL" reftype="world" refname="world"/>

    <framelinvel   name="velocity_trunk" objtype="body" objname="trunk" reftype="world" refname="world"/>
    <frameangvel   name="ang_velocity_trunk" objtype="body" objname="trunk" reftype="world" refname="world"/>
  </sensor>

</mujoco>
